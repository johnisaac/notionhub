<table id="feedbacks" cellpadding="0" cellspacing="0">
	<%if !@feedbacks.nil?%>
		<b>Feedback Count: <%=h @feedbacks.count%></b>
	<%else%>
		<b>Feedback Count: 0</b>
	<%end%>
	<tr class="head">
		<td>Feedback Content</td>
		<td>Feedback Rating</td>
		<td>User</td>
		<td>Time</td>
	</tr>
	<%@feedbacks.each do |feedback|%>
	<tr>
		<%if !feedback.content.nil?%>
			<td class="content"><%=h feedback.content%></td>
		<%else%>
			<td class="content"></td>
		<%end%>
		<%if !feedback.rating.nil?%>
			<td class="rating"><%=h feedback.rating%></td>
		<%else%>
			<td class="rating"></td>
		<%end%>
		<%if !feedback.user_id.nil?%>
			<td class="user_id"><%=h feedback.user_id%></td>
		<%else%>
			<td class="user_id"></td>
		<%end%>
		<td class="time"><%=h feedback.created_at%></td>
	</tr>
	<%end%>
</table>